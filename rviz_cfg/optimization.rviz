Panels:
  - Class: rviz/Displays
    Help Height: 78
    Name: Displays
    Property Tree Widget:
      Expanded:
        - /Global Options[0]
        - /GPS Odometry (GPS位置)[0]
        - /Loop Local Map (局部地图-点云)[1]
        - /Loop Current Scan (当前帧点云)[2]
      Splitter Ratio: 0.5
    Tree Height: 751
Visualization Manager:
  Class: ""
  Displays:
    - Alpha: 1
      Class: rviz/Odometry
      Color: 238; 238; 236
      Enabled: true
      Keep: 1000
      Name: GPS Odometry (GPS位置)
      Orientation Tolerance: 0.1
      Position Tolerance: 0.1
      Shape:
        Alpha: 1
        Axes Length: 5
        Axes Radius: 2
        Value: Axes
      Topic: /optimization/gps_odom
      Value: true
    - Alpha: 1
      Class: rviz/PointCloud2
      Color: 0; 255; 0
      Color Transformer: FlatColor
      Enabled: true
      Name: Loop Local Map (局部地图-点云)
      Topic: /optimization/loop_local_map_cloud
      Size (m): 0.03
      Style: Points
      Value: true
    - Alpha: 1
      Class: rviz/PointCloud2
      Color: 255; 255; 0
      Color Transformer: FlatColor
      Enabled: true
      Name: Loop Current Scan (当前帧点云)
      Topic: /optimization/loop_current_scan
      Size (m): 0.05
      Style: Points
      Value: true
  Enabled: true
  Global Options:
    Background Color: 48; 48; 48
    Fixed Frame: map
    Frame Rate: 30
  Name: root
  Tools:
    - Class: rviz/Interact
      Hide Inactive Objects: true
    - Class: rviz/MoveCamera
    - Class: rviz/Select
    - Class: rviz/FocusCamera
    - Class: rviz/Measure
    - Class: rviz/SetInitialPose
      Topic: /initialpose
    - Class: rviz/SetGoal
      Topic: /move_base_simple/goal
    - Class: rviz/PublishPoint
      Single click: true
      Topic: /clicked_point
  Value: true
  Views:
    Current:
      Class: rviz/Orbit
      Distance: 35.0
      Focal Point:
        X: 0
        Y: 0
        Z: 0
      Name: Current View
      Near Clip Distance: 0.01
      Pitch: 0.785
      Target Frame: <Fixed Frame>
      Value: Orbit (rviz)
      Yaw: 0
Window Geometry:
  Displays:
    collapsed: false
  Height: 1025
  Hide Left Dock: false
  Hide Right Dock: true
  QMainWindow State: 
  Selection:
    collapsed: false
  Time:
    collapsed: false
  Tool Properties:
    collapsed: false
  Views:
    collapsed: true
  Width: 1845
  X: 75
  Y: 27